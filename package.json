{
  "name": "graphcool-graphiql",
  "version": "1.3.9",
  "main": "./babelbuild/Playground.js",
  "homepage": "http://graphcool-playground.netlify.com",
  "files": [
    "babelbuild",
    "base.css",
    "graphiql_light.css",
    "graphiql_dark.css"
  ],
  "devDependencies": {
    "@types/jest": "^16.0.4",
    "@types/node": "^7.0.0",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.20",
    "autoprefixer": "6.5.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "codemirror": "^5.22.2",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "17.0.2",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-inherit": "^4.0.3",
    "postcss-inject": "^1.0.1",
    "postcss-loader": "1.0.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "promise": "7.1.1",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "2.1.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "styled-jsx-postcss": "^0.1.5",
    "ts-loader": "^1.3.1",
    "tslint": "^4.0.2",
    "tslint-config-standard": "^2.0.0",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.0.0",
    "typescript": "^2.1.4",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "babel": {
    "plugins": [
      "styled-jsx-postcss/babel"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-stage-3": "^6.22.0",
    "calculate-size": "^1.1.1",
    "classnames": "^2.2.5",
    "cuid": "^1.3.8",
    "graphcool-styles": "0.0.98",
    "graphiql": "^0.8.1",
    "graphql": "^0.8.2",
    "isomorphic-fetch": "^2.2.1",
    "postcss-modules": "^0.6.4",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.2",
    "react-modal": "^1.6.5",
    "react-virtualized": "^8.11.3",
    "rxjs": "^5.0.3",
    "seamless-immutable": "^7.0.1",
    "subscriptions-transport-ws": "^0.4.0",
    "svg-loader": "^0.0.2",
    "svgo-loader": "^1.1.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build-app": "node scripts/build.js",
    "build": "rm -rf tsbuild; rm -rf babelbuild; tsc --jsx preserve --allowJs src/Playground.tsx --target es5 --lib es2015,dom --outDir tsbuild; babel tsbuild -d babelbuild; cp -r ./src/assets/ ./babelbuild/assets/",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
